<?xml version="1.0" encoding="utf-8"?>
<mime-type xmlns="http://www.freedesktop.org/standards/shared-mime-info" type="application/x-quiz">
  <comment>Quiz application file</comment>
  <glob pattern="*.quiz"/>
  <!-- Use the application icon so file managers show the same icon for .quiz files -->
  <icon name="quiz-app"/>
</mime-type>